let arr2 = new Array();
let arr3 = [1, "2", true, null, undefined, {}, [], function () {}]; // 배열 리터럴
// 자료형에 상관없이 다 넣을 수 있다.
let arr = [1, 2, 3, 4, 5];
console.log(arr);
console.log(arr[0]);
console.log(arr[1]);
console.log(arr[2]); // index 접근가능

// 배열 추가 push
arr.push({ key: "value" });
console.log(arr.length); // 현재 배열의 길이
arr.push("6") // 추추가 
